import std.Sys;

main() {
	Sys.println("Input two numbers on two seperate lines:");

	numStr1 := Sys.stdin.readLine();
	numStr2 := Sys.stdin.readLine();
	
	Sys.println();
	Sys.println("Read in numbers:");

	num1 := numStr1.parseInt();
	num2 := numStr2.parseInt();

	sum := num1 + num2;

	Sys.print("Sum: ");
	Sys.println(sum);

}