import std.Sys;
import std.util.Random;

main() {
	
	Sys.println("Guess a number between 0-10");
	guess :i32 = Sys.stdin.readInt();

	rand := Random();
	answer := rand.nextInt(10);

	if guess == answer {
		Sys.println("You guessed correctly!");
	} else {
		Sys.println("You guessed wrong!");
		Sys.print("Right answer was: ");
		Sys.println(answer);
	}
}