import Tests;

// -- Test to make sure that nested
//    records work.
OuterRecord :: record {
	InnerRecord :: record {
		field1 :i32;
		field2 :i32;
		field3 :i32;
	}
}

main() {

	obj :OuterRecord.InnerRecord = OuterRecord.InnerRecord(
		field1 = 1415,
		field2 = 156,
		field3 = 76534,
	);

	Tests.print(obj.field1);
	Tests.print(" ");
	Tests.print(obj.field2);
	Tests.print(" ");
	Tests.print(obj.field3);
}