import Tests;

// -- Test to make sure that records
//    can contain another record as
//    a field.
A :: record {
	f1 :i32;
	f2 :i32;
}

Data :: record {
	a :A;
	b :i32;
}

main() {
	d :Data = Data(
		a = A(
			f1 = 124,
			f2 = 66
		),
		b = 25
	);

	Tests.print(d.a.f1);
	Tests.print(" ");
	Tests.print(d.a.f2);
	Tests.print(" ");
	Tests.print(d.b);
}