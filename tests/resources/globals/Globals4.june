import Tests;

// -- Test to make sure that arrays
//    of records work as global variables.
Data :: record {
	a: i32 = 154;
	b: i32 = 11;
}

arr: Data[5];

main() {
	Tests.print(arr[0].a);
	Tests.print(" ");
	Tests.print(arr[1].a);
	Tests.print(" ");
	Tests.print(arr[2].b);
	Tests.print(" ");
	Tests.print(arr[3].b);
	Tests.print(" ");
	Tests.print(arr[4].b);
}