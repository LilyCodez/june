import Tests;

// -- Test to make sure that calling
//    variable functions can be stored
//    inside of arrays and then called.
a() -> i32 {
	return 1241;
}

b() -> i32 {
	return 778;
}

main() {
	
	funcs :`() -> i32`[2];
	funcs[0] = &a;
	funcs[1] = &b;
	
	Tests.print(funcs[0]());
	Tests.print(" ");
	Tests.print(funcs[1]());
}